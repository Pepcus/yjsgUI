{
"memberCredentialFormConfig": {
  "schema": {
    "type": "object",
    "properties": {
      "memberId": {
        "title": "User Name",
        "type": "string"
      },
      "secretKey": {
        "title": "Password",
        "type": "string",
        "format": "password"
      }
    },
    "required": ["memberId", "secretKey"]
  },
  "uiSchema": {
    "memberId": {
      "ui:autofocus": true,
      "ui:field": "InputField",
      "ui:placeholder": "Enter your name",
      "ui:options": {
        "col": {
          "size": {
            "xs": 12,
            "sm": 12,
            "md": 6,
            "lg": 6
          },
          "padding": "0 10px 0 10px"
        }
      }
    },
    "secretKey": {
      "ui:field": "InputField",
      "ui:placeholder": "Enter your password",
      "ui:options": {
        "col": {
          "size": {
            "xs": 12,
            "sm": 12,
            "md": 6,
            "lg": 6
          },
          "padding": "0 10px 0 10px"
        }
      }
    }
  }
}
}
