{
  "registrationFormConfig": {
    "schema": {
      "type": "object",
      "required": [
        "fullName",
        "email",
        "phoneNumber",
        "numberOfPeople",
        "address",
        "myProducts"
      ],
      "properties": {
        "fullName": {
          "title": "Full Name",
          "type": "object",
          "boxBorder": true,
          "required": [
            "firstName",
            "lastName"
          ],
          "properties": {
            "firstName": {
              "title": "First Name",
              "type": "string",
              "customValidation": "nameValidator"
            },
            "lastName": {
              "title": "Last Name",
              "type": "string",
              "customValidation": "nameValidator"
            }
          }
        },
        "email": {
          "title": "E-mail",
          "type": "string",
          "customValidation": "optionalEmailValidator"
        },
        "phoneNumber": {
          "title": "Phone Number",
          "type": "object",
          "boxBorder": true,
          "required": [
            "areaCode",
            "phoneNumber"
          ],
          "properties": {
            "areaCode": {
              "title": "Area Code",
              "type": "number"
            },
            "phoneNumber": {
              "title": "Phone Number",
              "type": "number",
              "customValidation": "mobileValidator"
            }
          }
        },
        "numberOfPeople": {
          "title": "Number of people attending",
          "type": "number",
          "customValidation": "numberOfPeopleValidator"
        },
        "address": {
          "title": "Address",
          "type": "object",
          "boxBorder": true,
          "required": [
            "streetAddress",
            "streetAddressLine2",
            "city",
            "stateOrProvince",
            "postalOrZipCode",
            "country"
          ],
          "properties": {
            "streetAddress": {
              "title": "Street Address",
              "type": "string"
            },
            "streetAddressLine2": {
              "title": "Street Address Line 2",
              "type": "string"
            },
            "city": {
              "title": "City",
              "type": "string"
            },
            "stateOrProvince": {
              "title": "State / Province",
              "type": "string"
            },
            "postalOrZipCode": {
              "title": "Postal / Zip Code",
              "type": "string"
            },
            "country": {
              "title": "Country",
              "type": ["string","null"],
              "enum": [
                "United States",
                "Afghanistan",
                "Albania",
                "Algeria",
                "American Samoa",
                "Andorra",
                "Angola",
                "Anguilla",
                "Antigua and Barbuda",
                "Argentina",
                "Armenia",
                "Aruba",
                "Australia",
                "Austria",
                "Azerbaijan",
                "The Bahamas",
                "Bahrain",
                "Bangladesh",
                "India",
                "Indonesia"
              ]
            }
          }
        },
        "myProducts": {
          "title": "My Products",
          "type": "object",
          "boxBorder": true,
          "properties": {
            "option1": {
              "type": "boolean",
              "title": "Conference Full-Access w/ Dinner (member) $ 125.00",
              "default": false
            },
            "option2": {
              "type": "boolean",
              "title": " Conference Full-Access w/ Dinner (non-member) $ 150.00",
              "default": false
            }
          }
        }
      }
    },
    "uiSchema": {
