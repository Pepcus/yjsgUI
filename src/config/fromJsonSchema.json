  "UpdateIdCardStatusSelectedStudentsJsonSchema": {
    "Schema": {
      "title": "कृपया दिए गए ID's को  कार्ड प्रिंट के लिए चयन करें!",
      "type": "object",
      "properties": {
        "studentIds": {
          "title": "Selected Students Id:",
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "selectedCardOption": {
          "type": "string",
          "oneOf": [
            {
              "title": "Reprint",
              "const": "Y"
            },
            {
              "title": "Not Print",
              "const": "N"
            }
          ]
        },
        "close": {
          "type": "string"
        },
        "submit": {
          "type": "string"
        }
      },
      "required": ["studentIds", "selectedCardOption"]
    },
    "UISchema": {
      "ui:order": ["studentIds",
        "selectedCardOption",
        "close",
        "submit"
      ],
      "studentIds": {
        "className": "column-content-modal column-wrapper",
        "ui:options": {
          "addable": false,
          "removable": false
        },
        "items": {
          "ui:disabled": true
        }
      },
      "selectedCardOption": {
        "ui:options": {
          "label": false
        },
        "ui:widget": "radio"
      },
      "close": {
        "ui:options": {
          "label": false
        }
      },
      "submit": {
        "ui:options": {
          "label": false
        }
      }
    },
    "data": {}
  },
