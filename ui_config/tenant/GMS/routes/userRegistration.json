{
  "registrationFormConfig": {
    "schema": {
      "type": "object",
      "required": [
        "name",
        "age",
        "mobile",
        "city",
        "isWhatsApp",
        "address"
      ],
      "properties": {
        "name": {
          "title": "नाम",
          "type": "string",
          "customValidation": "nameValidator"
        },
        "age": {
          "title": "उम्र",
          "type": "string",
          "customValidation": "ageValidatorWithNoUpperLimit"
        },
        "mobile": {
          "title": "मोबाइल नं ( WhatsApp / SMS के लिये )",
          "type": "string",
          "customValidation": "mobileValidator"
        },
        "address": {
          "title": "पूरा पता",
          "type": "string",
          "customValidation": "blankValidator"
        },
        "city": {
          "title": "शहर",
          "type": "string",
          "enum": [
            "Indore",
            "Other"
          ],
          "enumNames": [
            "इंदौर",
            "कोई और शहर"
          ]
        },
        "isWhatsApp": {
          "title": "दीये गए नंबर पर WhatsApp उपयोग",
          "type": "string",
          "enum": [
            "true",
            "false"
          ],
          "enumNames": [
            "हाँ",
            "नहीं"
          ]
        }
      },
      "dependencies": {
        "city": {
          "oneOf": [{
            "properties": {
              "city": {
                "title": "शहर",
                "type": "string",
                "enum": [
                  "Other"
                ],
                "enumNames": [
                  "कोई और शहर"
                ]
              },
              "cityName": {
                "title": "शहर का नाम",
                "type": "string",
                "customValidation": "cityValidator"
              }
            },
            "required": [
              "cityName"
            ]
          }]
        }
      }
    },
    "uiSchema": {
      "ui:order": [
        "name",
        "age",
        "mobile",
        "address",
        "city",
        "cityName",
        "isWhatsApp"
      ],
      "name": {
        "ui:field": "InputField",
        "ui:options": {
          "style": {
            "fieldStyle": {
              "height": "auto"
            }
          },
          "type": "string",
          "col": {
            "size": {
              "xs": 12,
              "sm": 12,
              "md": 6,
              "lg": 4
            },
            "padding": "0 10px 0 10px"
          }
        }
      },
      "age": {
        "ui:field": "InputField",
        "ui:options": {
          "style": {
            "fieldStyle": {
              "height": "auto"
            }
          },
          "type": "string",
          "col": {
            "size": {
              "xs": 12,
              "sm": 12,
              "md": 6,
              "lg": 4
            },
            "padding": "0 10px 0 10px"
          }
        }
      },
      "mobile": {
        "ui:field": "InputField",
        "ui:disabled": true,
        "ui:options": {
          "style": {
            "fieldStyle": {
              "height": "auto"
            }
          },
          "type": "string",
          "col": {
            "size": {
              "xs": 12,
              "sm": 12,
              "md": 6,
              "lg": 4
            },
            "padding": "0 10px 0 10px"
          }
        }
      },
      "address": {
        "ui:field": "InputField",
        "ui:options": {
          "style": {
            "titleStyle": {
            },
            "fieldStyle": {
              "height": "35px"
            }
          },
          "type": "text",
          "col": {
            "size": {
              "xs": 12,
              "sm": 12,
              "md": 6,
              "lg": 4
            },
            "padding": "0 10px 0 10px"
          }
        }
      },
      "city": {
        "ui:field": "SelectList",
        "ui:placeholder": "कृपया एक चुनिए . . .",
        "ui:options": {
          "style": {
            "fieldStyle": {
              "height": "auto"
            }
          },
          "type": "string",
          "col": {
            "size": {
              "xs": 12,
              "sm": 12,
              "md": 6,
              "lg": 4
            },
            "padding": "0 10px 0 10px"
          }
        }
      },
      "cityName": {
        "ui:field": "InputField",
        "ui:options": {
          "style": {
            "fieldStyle": {
              "height": "auto"
            }
          },
          "type": "string",
          "col": {
            "size": {
              "xs": 12,
              "sm": 12,
              "md": 6,
              "lg": 4
            },
            "padding": "0 10px 0 10px"
          }
        }
      },
      "isWhatsApp": {
        "ui:field": "SelectList",
        "ui:placeholder": "कृपया एक चुनिए . . .",
        "ui:options": {
          "style": {
            "fieldStyle": {
              "height": "auto"
            }
          },
          "type": "string",
          "col": {
            "size": {
              "xs": 12,
              "sm": 12,
              "md": 6,
              "lg": 4
            },
            "padding": "0 10px 0 10px"
          }
        }
      }
    }
  }
}
