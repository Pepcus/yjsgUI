            "size": {
              "xs": 12,
              "sm": 12,
              "md": 6,
              "lg": 6
            },
            "padding": "0 10px 0 10px"
          }
        }
      },
      "adminPassword": {
        "ui:field": "InputField",
        "ui:placeholder": "Enter Admin Password",
        "ui:options": {
          "col": {
            "size": {
              "xs": 12,
              "sm": 12,
              "md": 6,
              "lg": 6
            },
            "padding": "0 10px 0 10px"
          }
        }
      }
    },
    "data": {}
  }
