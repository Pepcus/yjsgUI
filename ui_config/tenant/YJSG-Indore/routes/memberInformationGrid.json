        "ui:options": {
          "label": false
        }
      }
    }
  },
  "opInModalFormSchema": {
    "schema": {
      "type": "object",
      "properties": {
        "membersId": {
          "title": "Selected Students Id:",
          "type": "string"
        },
        "selectedOptOption": {
          "type": "string",
          "oneOf": [
            {
              "title": "हाँ",
              "const": "Y"
            },
            {
              "title": "ना",
              "const": "N"
            }
          ]
        }
      },
      "required": ["selectedOptOption"]
    },
    "uiSchema": {
      "ui:order": [
        "membersId",
        "selectedOptOption"
      ],
      "membersId": {
        "ui:disabled": true
      },
      "selectedOptOption": {
        "ui:options": {
          "label": false,
          "inline": true,
          "boxProps": {
            "margin-left": "20px",
            "padding": "10px 25px",
            "backgroundColor": "unset"
          }
        },
        "ui:widget": "radio"
      }
    }
  },
  "attendanceModalFormSchema": {
